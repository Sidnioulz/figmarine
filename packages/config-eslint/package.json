{
  "name": "@figmarine/config-eslint",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "description": "ESLint config package for the Figmarine monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sidnioulz/figmarine",
    "directory": "packages/config-eslint"
  },
  "scripts": {
    "build": "tsc"
  },
  "bugs": {
    "url": "https://github.com/Sidnioulz/figmarine/issues"
  },
  "keywords": [
    "config",
    "eslint"
  ],
  "author": "Steve Dodier-Lazaro",
  "homepage": "https://github.com/Sidnioulz/figmarine",
  "files": [
    "*.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./*.js"
    }
  },
  "dependencies": {
    "@eslint/json": "^0.11.0",
    "eslint-config-turbo": "^2.4.4",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "typescript-eslint": "^8.27.0"
  },
  "devDependencies": {
    "@vitest/eslint-plugin": "^1.1.38",
    "typescript": "^5.8.2"
  }
}