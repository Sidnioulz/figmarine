{
  "name": "@figmarine/config-eslint",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "description": "ESLint config package for the Figmarine monorepo",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sidnioulz/figmarine",
    "directory": "packages/config-eslint"
  },
  "scripts": {
    "build": "tsc"
  },
  "bugs": {
    "url": "https://github.com/Sidnioulz/figmarine/issues"
  },
  "keywords": [
    "config",
    "eslint"
  ],
  "author": "Steve Dodier-Lazaro",
  "homepage": "https://github.com/Sidnioulz/figmarine",
  "files": [
    "*.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./*.js"
    }
  },
  "dependencies": {
    "@eslint/json": "^0.12.0",
    "eslint-config-turbo": "^2.5.3",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "typescript-eslint": "^8.32.1"
  },
  "devDependencies": {
    "@vitest/eslint-plugin": "^1.1.44",
    "typescript": "^5.8.3"
  }
}