    "@figmarine/config-prettier": "workspace:*",
    "@figmarine/config-tsup": "workspace:*",
    "@figmarine/config-typescript": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.0",
    "jest": "^29.7.0",
    "tsup": "^8.2.4",
    "typescript": "latest"
  },
  "dependencies": {
    "@figmarine/logger": "workspace:*",
